    "@react-navigation/stack": "^6.0.7",
    "axios": "^0.21.4",
    "date-fns": "^2.24.0",
    "lodash": "^4.17.21",
    "native-base": "^3.2.1-rc.2",
    "node-html-parser": "^4.1.4",
    "react": "18.2.0",
    "react-native": "0.73.5",
    "react-native-code-push": "^8.2.1",
